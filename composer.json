{
    "name": "deckdom/dynql",
    "version": "1.0.0",
    "type": "library",
    "description": "A simple library to dynamically resolve GraphQL Fragments",
    "keywords": ["graphql", "dynamic", "fragment"],
    "homepage": "https://github.com/deckdom/dynql-php",
    "license": "MIT",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/deckdom/dynql-php"
    }],
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Dominik Decker",
            "email": "mail@deckdom.at"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/phpcov": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Dynql\\": "/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynql\\Tests\\": "/tests/"
        }
    }
}
